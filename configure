cd src
rm -rf generated
mkdir -p generated
cd ../dia

#for f in *.dia;
#do
#dia2code $f -d ../src/generated -nc
#done

dia2code state.dia -d ../src/generated -nc
dia2code render.dia -d ../src/generated -nc
sed -i '/#include "state.h/d' ../src/generated/render.h #ligne à supprimer lorsque la generation de state.h sera correct
sed -i '/#include "sf.h/d' ../src/generated/render.h #ligne à supprimer lorsque la generation de state.h sera correct
rm ../src/generated/sf.h
dia2code gui.dia -d ../src/generated -nc
dia2code guiRender.dia -d ../src/generated -nc
dia2code engine.dia -d ../src/generated -nc
dia2code ai.dia -d ../src/generated -nc
dia2code client.dia -d ../src/generated -nc

cd ..	

rm -rf bin
rm -rf build
mkdir -p bin
mkdir -p build
cd build
cmake ..

